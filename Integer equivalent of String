import java.io.*;
import java.util.*;

/*
 * Integer equivalent of String
 */

class Solution {
  public static void main(String[] args) {
    
    String s= "Hello";
    int result = 0;
    for (int i = 0; i < s.length(); i++) {
        result += Math.pow(10, i) * s.charAt(s.length() - i - 1);
    }
     System.out.println(result);
  }
}
